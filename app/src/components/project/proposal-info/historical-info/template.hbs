				role="button">{{t "project.proposal-info.product-info.produce-sale-report"}}</span>
		{{/bp-layout}}
		{{#bp-layout tagName="li" cross="center" local-class=(if (bp-eq salesGroupValue 1) "active") classNames="px-4"}}
