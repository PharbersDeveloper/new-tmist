{{common/nav-top-project proposalName=model.proposal.name}}
{{#bp-layout tagName="span"  classNames="ml-4 mt-4 mb-3" main="start" cross="start"}}
    {{#bp-layout local-class="return" click=(action "returnPrepare" model.proposal.id)}}返回{{/bp-layout}}
{{/bp-layout}}
{{#bp-layout direction="col" main="start" cross="center" local-class="history" classNames="px-4 pb-4"}}
    {{#bp-layout direction="col" main="start" cross="start" local-class="list-wrapper" classNames="p-4 bg-white"}}
        {{#bp-layout direction="row" main="start" cross="center" local-class="title-wrapper" classNames="mb-4"}}
            <span local-class="badge">
                {{#if (bp-eq model.proposal.case "tm")}}
                    TMIST
                {{else if (bp-eq model.proposal.case "ucb") }}
                    UCB
                {{else}}
                    undefined
                {{/if}}
            </span>
            <p local-class="title" class="ml-2">历史成果</p>
        {{/bp-layout}}

        {{!-- {{#bp-layout tagName="p" local-class="no-task" cross="center" main="center"}}
            还没有完成过该任务
        {{/bp-layout}} --}}

        {{project/history-info provious=model.provious}}
        
    {{/bp-layout}}
{{/bp-layout}}

