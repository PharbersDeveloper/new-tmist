{{common/nav-top-project proposalName=model.project.proposal.name noNavButton=noNavButton}}
{{#bp-layout direction="col" classNames="p-4" local-class="round-over"}}
	{{#bp-layout local-class="congratulations" direction="row" main="between"}}
		{{#bp-layout direction="row" cross="center"}}
			{{bp-icon icon="check-circle" local-class="icon"}}
			{{#bp-layout classNames="ml-3" local-class="text"}}恭喜你，完成本轮测试！{{/bp-layout}}
		{{/bp-layout}}
		{{#bp-layout}}
			{{#bp-button type="primary" onClick=(action "toIndex")}}返回主页{{/bp-button}}
		{{/bp-layout}}
	{{/bp-layout}}
	{{#if ( bp-eq model.project.proposal.case "ucb")}}
	{{#bp-layout local-class="info ucb-info" direction="row" classNames="bg-white p-4"}}
		{{!-- {{#bp-layout direction="col" local-class="name block"}}
			{{#bp-layout local-class="header-text"}}获得称号{{/bp-layout}}
			{{#bp-layout local-class="img"}}{{/bp-layout}}
			{{#bp-layout}}黄金{{/bp-layout}}
		{{/bp-layout}} --}}
		{{#bp-layout local-class="goal block" direction="col" cross="start" }}
			{{#bp-layout local-class="header-text" classNames="mb-3"}}总指标达成率{{/bp-layout}}
			{{#bp-layout direction="row" local-class="precent" cross="center"}}
				{{!-- <span>{{bp-percent model.report.quotaAchv}}</span> --}}
				<span>{{calc-quota-achv model.reports}}</span>
				<span>%</span>
			{{/bp-layout}}
			{{!-- {{#bp-layout direction="row" main="between"}}
				{{#bp-layout direction="col" }}
					{{#bp-layout local-class="period"}}2019Q1{{/bp-layout}}
					{{#bp-layout}}60%{{/bp-layout}}
				{{/bp-layout}}
				{{#bp-layout direction="col"}}
					{{#bp-layout local-class="period"}}2019Q1{{/bp-layout}}
					{{#bp-layout}}60%{{/bp-layout}}
				{{/bp-layout}}
				{{#bp-layout direction="col"}}
					{{#bp-layout local-class="period"}}2019Q1{{/bp-layout}}
					{{#bp-layout}}60%{{/bp-layout}}
				{{/bp-layout}}
			{{/bp-layout}} --}}
		{{/bp-layout}}
		{{#bp-layout local-class="sales block" direction="col" cross="end"}}
			{{#bp-layout local-class="header-text" classNames="mb-3"}}总销售额{{/bp-layout}}
			{{#bp-layout}}¥{{calc-total-sales model.reports}}{{/bp-layout}}
			{{!-- {{#bp-layout}}¥{{format-number model.report.sales 0}}{{/bp-layout}} --}}
		{{/bp-layout}}
		{{#bp-layout local-class="blank-part"}}
		{{/bp-layout}}
		{{#bp-layout direction="col" local-class="time block" cross="start"}}
			{{#bp-layout local-class="header-text" classNames="mb-3"}}完成用时{{/bp-layout}}
			{{#bp-layout}}{{time-difference this.model.project.startTime this.model.project.endTime}}{{/bp-layout}}
			{{!-- {{#bp-layout local-class="header-text"}}完成时间{{/bp-layout}}
			{{#bp-layout}}2019-04-08 18:40{{/bp-layout}} --}}
		{{/bp-layout}}
		{{#bp-layout direction="col" local-class="time block" cross="center"}}
			{{!-- {{#bp-layout local-class="header-text"}}完成用时{{/bp-layout}}
			{{#bp-layout}}1小时30分钟{{/bp-layout}} --}}
			{{#bp-layout local-class="header-text" classNames="mb-3"}}完成时间{{/bp-layout}}
			{{#bp-layout}}{{format-date-standard this.model.project.endTime}}{{/bp-layout}}
		{{/bp-layout}}
		{{!-- {{#bp-layout local-class="block"}}
		{{/bp-layout}} --}}
		{{#bp-layout local-class="report" cross="center" direction="row" 
			cross="center" main="center"}}
			{{#bp-layout local-class="report" direction="row" main="center"
			 click=(action "toReport")}}
				周期销售报告
			{{/bp-layout}}
		{{/bp-layout}}

		{{!-- {{#bp-layout local-class="examine-export" cross="center" direction="row" cross="center" main="center"}}
			{{#bp-layout local-class="examine" direction="row" classNames="mr-4"}}
			{{/bp-layout}}
			{{#bp-layout local-class="export" direction="row"}}
				{{#bp-button click=(action "exportInput" pr.project)}}
					{{bp-icon icon="export" local-class="icon"}}导出输入
				{{/bp-button}}
			{{/bp-layout}}
		{{/bp-layout}} --}}
		{{#bp-layout local-class="examine-export" cross="center" direction="row" cross="center" main="center"}}
			{{!-- {{#bp-layout local-class="examine" direction="row" classNames="mr-4"}}
			{{/bp-layout}}
			{{#bp-layout local-class="export" direction="row"}}
				{{#bp-button click=(action "exportReport")}}
					{{bp-icon icon='export'}}
					<span class="ml-1">导出报告</span>
				{{/bp-button}}
			{{/bp-layout}} --}}

		{{#bp-dropdown local-class="btn-list" widthSize="auto" as |dd|}}
			{{#dd.button}}
				导出
				{{bp-icon icon='down'}}
			{{/dd.button}}
			{{#dd.menu as |menu|}}
				{{#menu.option click=(action "exportInput" pr.project)}}决策回顾{{/menu.option}}
				{{#menu.option click=(action "exportReport")}}周期销售报告{{/menu.option}}
			{{/dd.menu}}
		{{/bp-dropdown}}
		{{/bp-layout}}


	{{/bp-layout}}
	{{else if (bp-eq model.project.proposal.case "tm")}}
		{{#bp-layout local-class="info tm-info" direction="row" classNames="bg-white p-4"}}
			{{#bp-layout direction="col" local-class="name block" }}
					{{#bp-layout local-class="get-name" direction="row" main="center" classNames="mb-3"}}获得称号{{/bp-layout}}
					{{#bp-layout local-class="img"}}{{/bp-layout}}
					{{#bp-layout local-class="name-level" main="center" direction="row" classNames="mt-2"}}{{curProjectLevel}}{{/bp-layout}}
			{{/bp-layout}}
			{{#bp-layout local-class="goal block" direction="col" cross="start"}}
				{{#bp-layout local-class="header-text" classNames="mb-3"}}总指标达成率{{/bp-layout}}
				{{#bp-layout direction="row" local-class="precent" cross="center"}}
					<span class="mb-2">{{two-number-percent model.tmReports.lastObject.sales model.tmReports.lastObject.salesQuota}}</span>
					{{!-- <span></span> --}}
				{{/bp-layout}}
				{{!-- {{#bp-layout direction="row"}}
						{{#bp-layout direction="col" classNames="mr-3"}}
							{{#bp-layout local-class="period"}}2019Q1{{/bp-layout}}
							{{#bp-layout}}60%{{/bp-layout}}
						{{/bp-layout}}
						{{#bp-layout direction="col" classNames="mr-3"}}
							{{#bp-layout local-class="period"}}2019Q1{{/bp-layout}}
							{{#bp-layout}}60%{{/bp-layout}}
						{{/bp-layout}}
						{{#bp-layout direction="col" classNames="mr-3"}}
							{{#bp-layout local-class="period"}}2019Q1{{/bp-layout}}
							{{#bp-layout}}60%{{/bp-layout}}
						{{/bp-layout}}
					{{/bp-layout}} --}}
			{{/bp-layout}}

			{{#bp-layout local-class="sales block" direction="col" cross="end"}}
				{{#bp-layout local-class="header-text" classNames="mb-3"}}总销售额{{/bp-layout}}
				{{#bp-layout local-class="content-row" cross="center"}}¥{{format-number model.tmReports.lastObject.sales 0}}{{/bp-layout}}
			{{/bp-layout}}
			{{#bp-layout local-class="blank-part"}}
			{{/bp-layout}}

			{{#bp-layout direction="col" local-class="time block" cross="start"}}
				{{#bp-layout local-class="header-text" classNames="mb-3"}}完成用时{{/bp-layout}}
				{{#bp-layout local-class="content-row" cross="center"}}{{time-difference this.model.project.startTime this.model.project.endTime}}{{/bp-layout}}
			{{/bp-layout}}
			{{#bp-layout direction="col" local-class="time block" cross="center"}}
				{{#bp-layout local-class="header-text" classNames="mb-3"}}完成时间{{/bp-layout}}
				{{#bp-layout local-class="content-row" cross="center"}}{{format-date-standard this.model.project.endTime}}{{/bp-layout}}
			{{/bp-layout}}
			{{!-- {{#bp-layout direction="col" local-class="block"}} --}}
			{{!-- {{/bp-layout}} --}}

			{{!-- {{#bp-layout local-class="block"}}
				{{/bp-layout}} --}}
			
			{{!-- {{#bp-layout local-class="report block" direction="row" main="center"}}
				{{#bp-layout local-class="report" direction="row" classNames="mr-4" 
					 click=(action "toReport")}}
					周期销售报告
				{{/bp-layout}}
			{{/bp-layout}} --}}

			{{#bp-layout local-class="tm-examine-export"  direction="row" main="center" cross="center"}}
				{{#bp-layout local-class="examine" direction="row"}}
					{{#bp-dropdown local-class="btn-list" widthSize="auto" as |dd|}}
							{{#dd.button }}
								查看
								{{bp-icon icon='down'}}
							{{/dd.button}}
							{{#dd.menu as |menu|}}
								{{#menu.option click=(action "showTestPerformance")}}测试综合表现{{/menu.option}}
								{{#menu.option click=(action "toReport")}}周期销售报告{{/menu.option}}
								{{#menu.option click=(action "toDecisionReview")}}决策回顾{{/menu.option}}
							{{/dd.menu}}
						{{/bp-dropdown}}
				{{/bp-layout}}
				{{#bp-layout local-class="export" direction="row"}}
					{{!-- {{#bp-button click=(action "exportReport")}}
						{{bp-icon icon='export'}}
						<span class="ml-1">导出报告</span>
					{{/bp-button}} --}}
					{{#bp-dropdown local-class="btn-list" widthSize="auto" as |dd|}}
							{{#dd.button }}
								导出
								{{bp-icon icon='down'}}
							{{/dd.button}}
							{{#dd.menu as |menu|}}
								{{!-- {{#menu.option}}测试综合表现{{/menu.option}} --}}
								{{#menu.option click=(action "exportReport")}}周期销售报告{{/menu.option}}
								{{#menu.option click=(action "exportInput" pr.project)}}决策回顾{{/menu.option}}
							{{/dd.menu}}
						{{/bp-dropdown}}
				{{/bp-layout}}
			{{/bp-layout}}
		{{/bp-layout}}
	{{/if}}

{{#if (bp-not-eq model.proviousReport.length 0)}}
	{{#bp-layout local-class="history" classNames="bg-white mt-4 px-4" direction="col"}}
		{{#bp-layout tagName="h4" local-class="history-header" cross="center" classNames="mb-0"}}
			历史成果
		{{/bp-layout}}
		{{project/history-info provious=model.provious proviousReport=model.historyReports roundOver=roundOver evaluations=model.evaluations}}
	{{/bp-layout}}
{{/if}}

{{/bp-layout}}




{{#bp-modal
	local-class="testPerformance"
	open=testPerformance
	position='center'
	onHidden=(action (mut testPerformance) false) as |modal|}}
	{{#modal.header closeButton=false classNames="px-4 pb-0 pt-4"}}
		{{#bp-layout direction="row" main="end" classNames="header-wrapper"}}
			{{#bp-layout direction="row" main="end" click=(action (mut testPerformance) false)}}
				{{bp-icon icon='cross'}}
			{{/bp-layout}}
		{{/bp-layout}}
	{{/modal.header}}
	{{#modal.body classNames="p-4"}}
	{{project/comprehensive-performance  project=this.model.project proviousReport=model.proviousReport evaluations=model.evaluations}}
	{{/modal.body}}
	{{!-- {{#modal.footer classNames="pt-0 pb-4 px-4"}}
		{{#bp-button type="primary" onClick=(action modal.close)}}好的{{/bp-button}}
		{{#bp-button type="subtle" onClick=(action modal.close)}}取消{{/bp-button}}
	{{/modal.footer}} --}}
{{/bp-modal}}

