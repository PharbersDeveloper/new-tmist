{{#common/nav-top-project proposalName=model.project.proposal.name}}
	{{#bp-layout direction="row"}}
		{{#if (bp-eq model.project.proposal.case "tm")}}
			{{#bp-button local-class="btn-submit" type="primary" onClick=(action "goRoundOver")}}完成任务{{/bp-button}}
		{{else if (bp-eq model.project.proposal.case "ucb")}}
			{{#if (bp-eq model.project.status 1)}}

				{{#if roundOver}}
				{{else}}
				{{#bp-button local-class="btn-submit" type="primary" onClick=(action "goRoundOver")}}完成任务{{/bp-button}}
				{{/if}}
			{{else}}
				{{#bp-button local-class="btn-submit" type="primary" onClick=(action "toNext" model.project)}}进行下个周期{{/bp-button}}
			{{/if}}
		{{/if}}
	{{/bp-layout}}
	{{!-- {{#bp-button local-class="btn-submit" type="primary" onClick=(action "testR")}}测试提交R{{/bp-button}} --}}
{{/common/nav-top-project}}

{{project/result-info
	project=model.project
	proposal=model.proposal
	resources=model.resources
	products=model.products
	hospitals=model.hospitals
	periods=model.periods
	reports=model.reports 
	summary=model.summary
	evaluations=model.evaluations 
	roundOver=roundOver
	yoy=model.yoy
	mom=model.mom
	goRoundOver=(action "goRoundOver")
	transitionToReport=(action "toReport")}}